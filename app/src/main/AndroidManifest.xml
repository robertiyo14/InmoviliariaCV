<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.izv.inmobiliariacv" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <provider
            android:name="com.izv.inmoviliariacv.Proveedor"
            android:authorities="com.izv.inmobiliariacv.proveedor"
            android:enabled="true"
            android:exported="true" >
        </provider>
        <activity
            android:name="com.izv.inmoviliariacv.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.izv.inmoviliariacv.Anadir"
            android:label="@string/title_activity_anadir" >
        </activity>
        <activity
            android:name="com.izv.inmoviliariacv.Fotos"
            android:label="@string/title_activity_fotos" >
        </activity>
    </application>

</manifest>
